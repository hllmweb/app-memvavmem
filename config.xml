<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.memvavmem" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MVM</name>
    <description>
        Agenda de contato do sistema memvavmem
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <icon platform="android" qualifier="ldpi" src="icon.png" />
    <icon platform="android" qualifier="mdpi" src="icon.png" />
    <icon platform="android" qualifier="hdpi" src="icon.png" />
    <icon platform="android" qualifier="xhdpi" src="icon.png" />
    <icon platform="android" qualifier="xxhdpi" src="icon.png" />
    <icon platform="android" qualifier="fr-xxhdpi" src="icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.3" />
</widget>
